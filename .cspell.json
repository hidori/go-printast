{
  "version": "0.2",
  "language": "en",
  "ignorePaths": [
    ".cspell.json",
    ".golangci.yml"
  ],
  "words": [
    "printast",
    "hidori",
    "hiroaki",
    "interactor",
    "shibuki",
    "struct",
    "usecase"
  ],
  "languageSettings": [
    {
      "languageId": "go",
      "allowCompoundWords": false,
      "includeRegExpList": [
        "CStyleComment",
        "string"
      ],
      "ignoreRegExpList": [
        "import\\s*\\((.|[\r\n])*?\\)",
        "import\\s*.*\".*?\"",
        "//\\s*go:generate.*",
        "//\\s*nolint:.*",
        "//\\s*cSpell:.*"
      ]
    }
  ]
}
